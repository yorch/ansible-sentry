[program:sentry-web]
directory = {{ app_root }}
command = {{ app_env }}/bin/sentry --config={{ sentry_conf_file }} start http
autostart = true
autorestart = true
redirect_stderr = true

[program:sentry-worker]
directory = {{ app_root }}
command = {{ app_env }}/bin/sentry --config={{ sentry_conf_file }} celery worker -B
autostart = true
autorestart = true
redirect_stderr = true
